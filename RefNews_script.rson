          "Total.Lines":  37,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Чтобы не перемешивать, отдельный блок для ивента \"Финансовый листок\"",
              "if(CurTurn() > 300) {",
              "    if(ControlledSystems(0) > 0) {",
              "        date = CurTurn() - 300;",
              "        if(date % 365 == 0){",
              "            SUCRN = ControlledSystems(0); //SUCRN - Systems Under Coalition Right Now, SUCYA - ~ Year Ago.",
              "            if(SUCYA != 0) {",
              "                if(SUCYA > SUCRN) {",
              "                    VPKpart = (CT('FinancicalNews.NewsVPK3'));",
              "                }",
              "                if(SUCYA < SUCRN) {",
              "                    VPKpart = (CT('FinancicalNews.NewsVPK1'));",
              "                }",
              "                if(SUCYA = SUCRN) {",
              "                    VPKpart = (CT('FinancicalNews.NewsVPK2'));",
              "                }",
              "                SUCYA = SUCRN;",
              "            }",
              "            AllSystems = 70;",
              "            if(CurrentMods(AndromedaGalaxyUpgrade) != 0) AllSystems = 150;",
              "            if(CurrentMods(AndromedaGalaxyUpgradeExtended) != 0) AllSystems = 200;",
              "            if(CurrentMods(RefGalaxyUpgrade) != 0) AllSystems = 150;",
              "            if(CurrentMods(RefGalaxyUpgradeExtended) != 0) AllSystems = 200;",
              "            AllSystems = AllSystems * 100;",
              "            SUPRN = ControlledSystems(2); //Systems Under Pirates Right Now",
              "            if(AllSystems % SUCRN >= 25) Creditspart = (CT('FinancicalNews.NewsCredits1'));",
              "            if(AllSystems % SUPRN >= 50) Creditspart = (CT('FinancicalNews.NewsCredits2'));",
              "            if(AllSystems % SUCRN < 25) Creditspart = (CT('FinancicalNews.NewsCredits3'));",
              "            if(GalaxyMoney = 0) Minfinpart = (CT('FinancicalNews.NewsMinFin1'));",
              "            if(GalaxyMoney = 1) Minfinpart = (CT('FinancicalNews.NewsMinFin2'));",
              "            if(GalaxyMoney = 2) Minfinpart = (CT('FinancicalNews.NewsMinFin3'));",
              "            if(GalaxyMoney = 3) Minfinpart = (CT('FinancicalNews.NewsMinFin4'));",
              "            if(GalaxyMoney = 4) Minfinpart = (CT('FinancicalNews.NewsMinFin5'));",
              "            AddPlanetNews(Format(CT('FinancicalNews.NewsMagazine'),'#VPK#',VPKpart,'#Credit#',Creditspart,'#Minfin#',Minfinpart));",
              "        }",
              "    }",
              "}"          
          ]        
        },
