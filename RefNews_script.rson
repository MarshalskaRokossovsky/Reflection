              "                            BombingEvent = 1;",
              "                            FromLastNews = 0;",
              "                        }",
              "                        else {",
              "                            AddPlanetNews(Format(CT('WarNews.NewsTerract'),'#pl1#',WarPlanet));",
              "                            FromLastNews = 0;",
              "                        }",
              "                    }",
              "                }",
              "                if(FromLastNews <= 300) {",
              "                    l = Rnd(30,95);",
              "                    if(l > 50) {",
              "                        if(BombingEvent == 0) {",
              "                            AddPlanetNews(Format(CT('WarNews.NewsBomb'),'#pl1#',WarPlanet));",
              "                            BombingEvent = 1;",
              "                            FromLastNews = 0;",
              "                        }",
              "                        else {",
              "                            AddPlanetNews(Format(CT('WarNews.NewsTerract'),'#pl1#',WarPlanet));",
              "                            FromLastNews = 0;",
              "                        }",
              "                    }",
              "                }",
              "                else {",
              "                    l = Rnd(45,100);",
              "                    if(l > 50) {",
              "                        if(BombingEvent == 0) {",
              "                            AddPlanetNews(Format(CT('WarNews.NewsBomb'),'#pl1#',WarPlanet));",
              "                            BombingEvent = 1;",
              "                            FromLastNews = 0;",
              "                        }",
              "                        else {",
              "                            AddPlanetNews(Format(CT('WarNews.NewsTerract'),'#pl1#',WarPlanet));",
              "                            FromLastNews = 0;",
              "                        }",
              "                    }",
              "                }",
              "            }",
              "            if(PeopleSupport <= 30) {",
              "                SU = Rnd(15 + PeopleSupport,100);",
              "                if(SU <= 50) {",
              "                    AddPlanetNews(Format(CT('WarNews.NewsEndRevolution'),'#pl1#',WarPlanet));",
              "                    PlanetGov(WarPlanet,4);",
