      "Operations":      
      [
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  1030,
          "Pos.y":  710,
          "Parent":  -1,
          "#":  0,
          "Total.Lines":  85,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if(GroupCount(PiratesWave1) < 1 && GroupCount(PiratesWave2) < 1 && GroupCount(PiratesWave3) < 1 && GroupCount(PirateShu) < 1 && holestate2 == 1){",
              "    holestate2 = 0;",
              "    SFT('REFLECTION-PIRATE-BLACKHOLE-Timer');",
              "    nextattack=CurTurn()+Rnd(500,700);",
              "    zatycka = 0;",
              "    holestate2=0;",
              "    idhh = 0;",
              "    hHolePos1 = 0;",
              "    hHolePos2 = 0;",
              "    tHolePos1 = 0;",
              "    tHolePos2 = 0;",
              "    timehole=0;",
              "    timehole2=0;",
              "    attackwant=0;",
              "    shudial=0;",
              "}",
              "//Перебираем все пиратские системы",
              "//Отвечает за формирование атаки",
              "if(CurTurn() >= nextattack && holestate2 == 0) {",
              "    SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION');",
              "    zatycka = 0;",
              "    holestate2=1;",
              "    idhh = 0;",
              "    hHolePos1 = 0;",
              "    hHolePos2 = 0;",
              "    tHolePos1 = 0;",
              "    tHolePos2 = 0;",
              "    timehole=0;",
              "    timehole2=0;",
              "    attackwant=0;",
              "    shudial=0;",
              "    FormAttackHoleAndFindSystems();",
              "    if(attackwant == 1){",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN');",
              "        planet = FindPlanet(GalaxyStar(tHolePos1-1),'NotNone',0,100);",
              "        //Генерируем первую волну",
              "        int f;",
              "        int rnd = Rnd(9,11);",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN-1');",
              "        for(f=0;f < rnd;f = f + 1){",
              "            tship = SpawnPirate(1,planet);",
              "            ShipJoin(PiratesWave1,tship);",
              "        }",
              "        rnd = Rnd(2,4);",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN2');",
              "        for(f=0;f < rnd;f = f + 1){",
              "            tship = SpawnPirate(2,planet);",
              "            ShipJoin(PiratesWave1,tship);",
              "        }",
              "        //Генерируем вторую волну",
              "        rnd = Rnd(6,8);",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN3');",
              "        for(f=0;f < rnd;f = f + 1){",
              "            tship = SpawnPirate(2,planet);",
              "            ShipJoin(PiratesWave2,tship);",
              "        }",
              "        rnd = Rnd(2,4);",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN4');",
              "        for(f=0;f < rnd;f = f + 1){",
              "            tship = SpawnPirate(3,planet);",
              "            ShipJoin(PiratesWave2,tship);",
              "        }",
              "        //Генерируем третью волну",
              "        rnd = Rnd(3,6);",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN5');",
              "        for(f=0;f < rnd;f = f + 1){",
              "            tship = SpawnPirate(3,planet);",
              "            ShipJoin(PiratesWave3,tship);",
              "        }",
              "        //ШУ",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start SESSION-SPAWN-SHU');",
              "        tship = SpawnPirate(4,planet);",
              "        ShipJoin(PirateShu,tship);",
              "    }",
              "    else {",
              "        SFT('REFLECTION-PIRATE-BLACKHOLE-Start-ATTACK-WANT-NO');",
              "        nextattack=CurTurn()+Rnd(10,30);",
              "    }",
              "",
              "}",
              "if(ShipInNormalSpace(Player())) {",
              "    if(PirateWin()==3 || PirateWin==4) {",
              "        AllShipOut();",
              "    }",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  1010,
          "Pos.y":  710,
          "Parent":  -1,
          "#":  1,
          "Total.Lines":  435,
          "Code.Type":  "Init",
          "Code":          
          [
              "nextattack=CurTurn()+Rnd(400,800);",
              "",
              "//Функция по созданию кораблей",
              "//type",
              "//1 = Спавним Лёгкого",
              "//2 = Спавним Среднего",
              "//3 = Спавним Тяжёлого",
              "//4 = Спавним Шу",
              "",
              "function SpawnPirate(int type,dword planet) {",
              "    //Запоминаем расу планеты чтоб потом её вернуть",
              "    int planetRace=PlanetRace(planet);",
              "    int planetOwner=PlanetOwner(planet);",
              "    //Переменая которая содержит в себе корабль",
              "    dword ship;",
              "    dword hullstat;",
              "    dword equip;",
              "    dword tequip;",
              "    //Определяет рандомную расу пирату.Для этого мы временно меняем рассу планете на которой делаем спавн",
              "    int size;",
              "    int rndspawner=1;",
              "    int texlvl = GalaxyTechLevel();",
              "    rndspawner = Rnd(0,4);",
              "",
              "    if(type == 1) {",
              "        PlanetRace(planet,rndspawner);",
              "        //Спавним Лёгкого",
              "        ship = BuyWarrior(planet,1000);",
              "",
              "        //Размер корпуса",
              "        ItemSize(ShipItems(ship,0),Rnd(280,400)+max(3,GalaxyTechLevel())*20);",
              "        //Навсякий случай назначаем также и хп",
              "        HullHP(ship,ItemSize(ShipItems(ship,0)));",
              "        Chameleon(ship);",
              "        hullstat=ShipItems(ship,0);",
              "        //Делаем корпус",
              "        if(GetEquipmentStats(hullstat,5)<1)ShipSpecialBonuses(ship,bonSlotDroid,1);",
              "        if(GetEquipmentStats(hullstat,7)<1)ShipSpecialBonuses(ship,bonSlotDef,1);",
              "        ShipSpecialBonuses(ship,bonSlotWeapon,5);",
              "        ShipSpecialBonuses(ship,bonSlotArt,4);",
              "        //Вешаем навыки",
              "        ShipSkill(ship,0,Rnd(3,6));",
              "        ShipSkill(ship,1,Rnd(5,6));",
              "        ShipSkill(ship,2,Rnd(2,6));",
              "        //Заставляем закупится корабль",
              "        for(i=0;i<3;i = i +1) {",
              "            ShipRefit(ship);",
              "        }",
              "        //Добавляем гарантировано гзп на гту выше",
              "        if(!ShipEqInSlot(ship,t_DefGenerator))",
              "        {",
              "            size = Rnd(15,25);",
              "            equip = CreateEquipment(t_DefGenerator,size,min(8,GalaxyTechLevel()+1),ShipOwner(ship));",
              "            AddItemToShip(ship,equip);",
              "            ItemIsInUse(equip,ship,1);",
              "        }",
              "        ShipSpecialBonuses(ship,bonSpeed,200);",
              "",
              "        for(i=0;i<4;i=i+1) {",
              "            if(i == 0){",
              "                equip=CreateArt(t_ArtPDTurret,ShipOwner(ship));",
              "            }",
              "            else if(i == 1){",
              "                equip=CreateArt(t_ArtefactMiniExpl,ShipOwner(ship));",
              "            }",
              "            else if(i == 2){",
              "                equip=CreateArt(t_ArtefactDef,ShipOwner(ship));",
              "            }",
              "            else {",
              "                equip=CreateArt(t_ArtSplinter,ShipOwner(ship));",
              "            }",
              "",
              "            ItemDurability(equip,100);",
              "            NoDropItem(equip,1);",
              "            AddItemToShip(ship,equip);",
              "            ItemIsInUse(equip,ship,1);",
              "        }",
              "",
              "    }",
              "    else if(type == 2) {",
              "        PlanetRace(planet,rndspawner);",
              "        //Спавним Среднего",
              "        ship = BuyWarrior(planet,1000);",
              "",
              "        //Размер корпуса",
              "        ItemSize(ShipItems(ship,0),Rnd(900,1500)+max(3,GalaxyTechLevel())*40);",
              "        //Навсякий случай назначаем также и хп",
              "        HullHP(ship,ItemSize(ShipItems(ship,0)));",
              "        Chameleon(ship);",
              "        hullstat=ShipItems(ship,0);",
              "        //Делаем корпус",
              "        if(GetEquipmentStats(hullstat,5)<1)ShipSpecialBonuses(ship,bonSlotDroid,1);",
              "        if(GetEquipmentStats(hullstat,7)<1)ShipSpecialBonuses(ship,bonSlotDef,1);",
              "        ShipSpecialBonuses(ship,bonSlotWeapon,5);",
              "        ShipSpecialBonuses(ship,bonSlotArt,4);",
              "        //Вешаем навыки",
